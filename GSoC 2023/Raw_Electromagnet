//This code is a basic example for using the Keyestudio Electromagnet Module (applies also to any other electromagnet)


int Solenoid = 0;    //define digital port 3 (D3 of ESP8266)
 void setup()  
 {        
  pinMode(Solenoid, OUTPUT);  //set Solenoid as OUTPUT
}
 void loop()                     
{
  digitalWrite(Solenoid, HIGH);// Solenoid is effective
  delay(2000); // delay 2S
  digitalWrite(Solenoid, LOW);  // Solenoid is non-effective
  delay(500);  //delay 2S      
}
